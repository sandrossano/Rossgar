<?xml version="1.0" encoding="utf-8"?>
<android.support.design.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/coordinator_app_bar"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.sandro.irsina.MainActivity">

    <android.support.design.widget.AppBarLayout
        android:id="@+id/appBarLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorAccent"
        android:theme="@style/AppTheme.AppBarOverlay">

        <android.support.v7.widget.Toolbar
            android:id="@+id/toolbar"
            android:layout_width="match_parent"
            android:layout_height="?attr/actionBarSize"
            android:background="@drawable/color_back"
            app:popupTheme="@style/AppTheme.PopupOverlay" />

    </android.support.design.widget.AppBarLayout>

    <include
        android:id="@+id/include_main"
        layout="@layout/content_main"
        android:visibility="visible" />

    <include
        android:id="@+id/include_main_2"
        layout="@layout/content_main_2"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="55dp"
        android:layout_marginTop="?attr/actionBarSize"
        android:visibility="gone" />

    <include
        android:id="@+id/include_cattedrale"
        layout="@layout/cattedrale"
        android:visibility="gone" />

    <include
        android:id="@+id/include_fuori"
        layout="@layout/fuori"
        android:visibility="gone" />

    <include
        android:id="@+id/include_fuori_2"
        layout="@layout/fuori_2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="55dp"
        android:layout_marginTop="56dp"
        android:visibility="gone" />

    <include
        android:id="@+id/include_sanfrancesco"
        layout="@layout/sanfrancesco"
        android:visibility="gone" />

    <include
        android:id="@+id/include_bottini"
        layout="@layout/bottini"
        android:visibility="gone" />

    <include
        android:id="@+id/include_pieta"
        layout="@layout/pieta"
        android:visibility="gone" />

    <include
        android:id="@+id/include_juso"
        layout="@layout/juso"
        android:visibility="gone" />

    <include
        android:id="@+id/include_peschiera"
        layout="@layout/peschiera"
        android:visibility="gone" />

    <include
        android:id="@+id/include_fontane"
        layout="@layout/fontane"
        android:visibility="gone" />

    <include
        android:id="@+id/include_sanfrancesco_cripta"
        layout="@layout/sanfrancescocripta"
        android:visibility="gone" />

    <include
        android:id="@+id/include_cripta"
        layout="@layout/cripta"
        android:visibility="gone" />

    <include
        android:id="@+id/include_museo"
        layout="@layout/museo"
        android:visibility="gone" />

    <include
        android:id="@+id/include_vedere_janora"
        layout="@layout/vedere_janora"
        android:visibility="gone" />

    <include
        android:id="@+id/include_nuget"
        layout="@layout/nuget1"
        android:visibility="gone" />

    <include
        android:id="@+id/include_porticella"
        layout="@layout/porticella1"
        android:visibility="gone" />

    <include
        android:id="@+id/include_muretto"
        layout="@layout/muretto1"
        android:visibility="gone" />

    <include
        android:id="@+id/include_catt_storia"
        layout="@layout/storia"
        android:visibility="gone" />

    <include
        android:id="@+id/include_sanfra_storia"
        layout="@layout/storia_sanfrancesco"
        android:visibility="gone" />

    <include
        android:id="@+id/include_sponsor"
        layout="@layout/content_sing"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="55dp"
        android:visibility="gone" />

    <include
        android:id="@+id/include_cancelle"
        layout="@layout/cancelle"
        android:visibility="gone" />

    <include
        android:id="@+id/include_catt_mantegna"
        layout="@layout/mantegna"
        android:visibility="gone" />

    <include
        android:id="@+id/include_catt_vedere"
        layout="@layout/vedere"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="55dp"
        android:visibility="gone" />

    <include
        android:id="@+id/include_catt_miglionico"
        layout="@layout/miglionico"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="55dp"
        android:visibility="gone" />

</android.support.design.widget.CoordinatorLayout>
